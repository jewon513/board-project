<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="SAMPLE"
		class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>[%d{yyyy-MM-dd HH:mm:ss}:%-3relative][%thread] %-5level %logger{35} - %msg%n</pattern>
		</encoder>
	</appender>

	<root level="DEBUG">
		<appender-ref ref="SAMPLE" />
	</root>


</configuration>